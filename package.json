{
  "name": "grape-learn",
  "version": "1.0.0",
  "description": "This project is aim to fill some gap that I have regarding some tech that grape is using.",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/server",
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "test": "jest",
    "build": "npm -s run clean && npm -s run generate && tsc && npm run build:schema",
    "build:schema": "ts-node src/schema/index.ts",
    "generate": "npm -s run generate:prisma",
    "generate:prisma": "prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "prisma": "^5.10.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@pothos/core": "^3.41.0",
    "@pothos/plugin-prisma": "^3.64.0",
    "@prisma/client": "^5.10.2",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.5",
    "graphql-yoga": "^5.1.1",
    "ts-node-dev": "^2.0.0"
  }
